{
	"name": "C# (.NET 8.0 - Ubuntu 20.04)",
	"build": {
		"dockerfile": "../Dockerfile",
		"context": "..",
		"args": {
			"VARIANT": "8.0-focal"
		}
	},
	"features": {
		"ghcr.io/devcontainers/features/common-utils:2": {},
		"ghcr.io/devcontainers/features/git:1": {}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-dotnettools.csharp",
				"ms-dotnettools.csdevkit"
			]
		}
	}
}
